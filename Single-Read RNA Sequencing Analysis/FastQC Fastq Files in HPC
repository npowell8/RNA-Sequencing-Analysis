#HPC used was AIRE

#!/bin/bash
#SBATCH --job-name=rnaseq         
#SBATCH --output=rnaseq.out        
#SBATCH --error=rnaseq.err         
#SBATCH --time=04:00:00            
#SBATCH --cpus-per-task=8          
#SBATCH --mem=16G                  

#State file path for outputs and directory of where fastq files are
fastqc -o /path/to/output/directory --threads 4 --dir /mnt/scratch/umnpo/report/temp /path/to/input/files/*.fastq.gz
