#HPC used was AIRE

#!/bin/bash
#SBATCH --job-name=rnaseq         
#SBATCH --output=rnaseq.out        
#SBATCH --error=rnaseq.err         
#SBATCH --time=04:00:00            
#SBATCH --cpus-per-task=8          
#SBATCH --mem=16G  

#Perform FASTQC on trimmed reads
fastqc -o /path/to/output/directory --threads 4 --dir /mnt/scratch/umnpo/ /path/to/input/files/*.fq
